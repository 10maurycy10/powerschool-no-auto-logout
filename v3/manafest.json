{
	"name": "Powerschool anti auto logout",
	"description": "Stops Power$chool from being annoying.",
	"version": "1.0",
	"manifest_version": 3,
	"icons": {"256": "icon256.png"},
	"permissions": [
		"activeTab",
		"scripting"
	],
	"content_scripts": [
		{
			"matches": ["*://*.powerschool.com/*"],
			"js": ["inject.js"]
		}
	],
	"actions": {}
}
